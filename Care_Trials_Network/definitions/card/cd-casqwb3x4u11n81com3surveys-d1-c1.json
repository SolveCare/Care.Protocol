        "3label0ess": "With",
        "3label1ness": "Without",

        "4tile0textq1": "4. Adequate space or available room for study monitoring visits",
        "4label0ess": "With",
        "4label1ness": "Without",

        "0tile0iitextpat": "Pathology:",

